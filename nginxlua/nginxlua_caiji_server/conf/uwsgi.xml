<uwsgi>  
<socket>127.0.0.1:10005</socket>  
<listen>20</listen>  
<master>true</master>  
<pidfile>/opt/nginx/uwsgi.pid</pidfile>  
<processes>8</processes>  
<module>zbapi</module>
<pythonpath>/opt/nginx/conf</pythonpath> 
<profiler>true</profiler>  
<enable-threads>true</enable-threads>  
<logdate>true</logdate>  
</uwsgi>
